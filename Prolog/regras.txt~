:- set_prolog_flag(encoding, utf8).

% :-dynamic (facto/2, ultimo_facto/1).

ultimo_facto(2).
ultima_regra(2).

regra 1
    se [pergunta(1, sim)]
    entao [cria_facto(conclusao(Ansiedade_como_fator_secundario))].

regra 2
    se [pergunta(2, nao)]
    entao [cria_facto(conclusao(Ansiedade_normal))].

facto(1,resposta(1, sim)).
facto(2,resposta(2, sim )).
